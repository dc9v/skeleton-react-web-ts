{
  "presets": [
    [
      "@babel/preset-env",
      {
        "useBuiltIns": "usage",
        "corejs": 3
      }
    ],
    [
      "@babel/preset-react"
    ],
    "@babel/preset-typescript"
  ],
  "plugins": [
    [
      "@babel/plugin-transform-runtime",
      {
        "regenerator": true
      }
    ],
    [
      "module-resolver",
      {
        "root": [
          "."
        ],
        "extension": [
          ".js",
          ".ts",
          ".tsx"
        ],
        "alias": {
          "/src": "./src"
        }
      }
    ]
  ]
}